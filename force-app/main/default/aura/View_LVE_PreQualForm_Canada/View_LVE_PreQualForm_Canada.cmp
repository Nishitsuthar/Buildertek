<aura:component Controller="View_LVE_PreQualForm_Canada_Controller" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
    <aura:attribute name="showSuccessMessage" type="Boolean" access="global" default="false"/>
    <aura:attribute name="isSuccess" type="Boolean" access="global"/>
    <aura:attribute name="isError" type="Boolean" access="global"/>
    <aura:attribute name="isRequired" type="Boolean" access="global" default="true"/>
    <aura:attribute name="isPreview" type="String" default="" />
    <aura:attribute name="Spinner" type="Boolean" default="false"/>
      <aura:attribute name="currFileIndex" type="Integer" default="0"/>
      <aura:attribute name="validationMessage" type="String" default=""/>
    
    <aura:attribute name="message" type="String" access="global"/>
    <aura:attribute name="isInputDisabled" type="Boolean" access="global" default="true"/>
    <aura:attribute name="pre" type="buildertek__Pre_Qual__c" default="{'sObjectType':'buildertek__Pre_Qual__c'}"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:attribute name="recordId" type="String" />
    
    <aura:attribute type="Date" name="contactDate"/>
    <aura:attribute name="provinceList" type="List"/>
    <aura:attribute name="countryList" type="List"/>
    
    
    <aura:attribute name="GeneralInfoFieldSet" type="List" access="private"/>
    <aura:attribute name="CompanyInfoFieldSet" type="List" access="private"/>
    <aura:attribute name="KeyManageMentSet" type="List" access="private"/>
    <aura:attribute name="LVEFieldSet" type="List" access="private"/>
    <aura:attribute name="ContractorFieldSet" type="List" access="private"/>
    
    <aura:attribute name="FileNameList" type="List" />
    <aura:attribute name="uploadFileList" type="List" />
    <aura:attribute name="FileLabelList" type="List" default="[]"/>
    <aura:attribute name="fileData" type="List" access="global"/>
    <aura:attribute name="uploadFile" type="Boolean" access="global"/>
    
    <aura:attribute name="LVEstimator" type="String" access="global"/>
    <aura:attribute name="AdditionalNotes" type="String" access="global"/>
    <aura:attribute name="SubmittedName" type="String" access="global"/>
    <aura:attribute name="SubmitteTitle" type="String" access="global"/>
    <aura:attribute name="SubmittedEmail" type="String" access="global"/>
    <aura:attribute name="FileUploadChecklist" type="String" access="global"/>
    
    
    <aura:attribute name="project1" type="buildertek__Past_Project__c" default="{'sObjectType':'buildertek__Past_Project__c'}"/>
    <aura:attribute name="project2" type="buildertek__Past_Project__c" default="{'sObjectType':'buildertek__Past_Project__c'}"/>
    <aura:attribute name="project3" type="buildertek__Past_Project__c" default="{'sObjectType':'buildertek__Past_Project__c'}"/>
    <aura:attribute name="reference1" type="buildertek__References__c" default="{'sObjectType':'buildertek__References__c'}"/>
    <aura:attribute name="reference2" type="buildertek__References__c" default="{'sObjectType':'buildertek__References__c'}"/>
    <aura:attribute name="reference3" type="buildertek__References__c" default="{'sObjectType':'buildertek__References__c'}"/>
    
    <aura:attribute name="check9" type="Boolean" default="false"/>
    <aura:attribute name="check2" type="Boolean" default="false"/>
    <aura:attribute name="check3" type="Boolean" default="false"/>
    <aura:attribute name="check4" type="Boolean" default="false"/>
    <aura:attribute name="check5" type="Boolean" default="false"/>
    <aura:attribute name="check6" type="Boolean" default="false"/>
    <aura:attribute name="check7" type="Boolean" default="false"/>
    <aura:attribute name="check8" type="Boolean" default="false"/>
    <aura:attribute name="isButtonDisabled" type="Boolean" default="true" />
    
    <aura:attribute name="Sendmecopy" type="Boolean" default="false"/>
    
    
    <aura:attribute name="validAllfields" type="Boolean" access="global" default="true"/> 
    
    
    <div style="background-color: white;padding-left:20px;">
        <aura:if isTrue="{!v.showSuccessMessage}">
            <div id="pageTop">
                <div>
                    <!--<span>
                        <img src="{!v.documentURL}" style="height: 30px;width: 125px;"/> 
                    </span>-->
                    <span class="slds-m-top_x-large">
                        <center>
                            <b>
                                <ul>
                                    <h2 style="font-size: 1.3rem;">
                                        Thank-You for submitting your information. The team will review your details and be in touch.
                                    </h2>
                                </ul>
                            </b>
                        </center>
                        <br/>
                    </span>
                    <div class="slds-align_absolute-center slds-m-top_small">
                        <lightning:button variant="neutral" label="Close" title="Close" onclick="{!c.closePage}" />
                    </div>
                </div>
            </div>
            
            <aura:set attribute="else">
                <div id="pageTop" class="slds-p-around_medium">
                    <aura:if isTrue="{!v.isSuccess}">
                        <div class="slds-notify slds-notify_alert slds-alert_success" role="alert" style="background-color:green">
                            <span class="slds-assistive-text">success</span>
                            <h2>
                                {!v.message}
                            </h2>
                            <div class="slds-notify__close">
                                
                            </div>
                        </div>
                    </aura:if>
                    <aura:if isTrue="{!v.isError}">
                        <div class="slds-notify slds-notify_alert slds-alert_error" role="alert">
                            <span class="slds-assistive-text">error</span>
                            <h2>
                                {!v.message}
                            </h2>
                            <div class="slds-notify__close">
                                
                            </div>
                        </div>
                    </aura:if>
                    
                    <div class="headerDiv slds-align_absolute-center">
                        <p style="text-align: center;font-size: 25px">
                            <b>LVE: Pre-qualification Form - Canada</b>
                        </p>  
                    </div>
                    <lightning:layout multipleRows="true">
                        <lightning:layoutItem size="12" padding="around-small">
                            <p  style="color:red;font-size: 14px;">
                                <b>Read Me First:</b>
                            </p> 
                        </lightning:layoutItem>
                        <p style="font-size: 13px; padding-left:8px;"> 
                            1. To prepare for submission review this form in its entirety to ensure you have all required content before completing.<br/>
                            2. See bottom of form for list of required forms.<br/>
                            3. This form must be completed in its entirety in order to submit the form.<br/>
                            4. Please add Not Applicable for any fields that do not apply to your business.
                        </p>
                    </lightning:layout>
                    
                    
                    
                    
                    <lightning:recordEditForm aura:id="recordEditForm"
                                              objectApiName="buildertek__Pre_Qual__c"
                                              onsubmit="{!c.handleSubmit}"
                                              onerror="{!c.handleError}">
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="2" padding="around-small">
                                <lightning:input label="Form Date Field" type="date" name="input1" value="{!v.contactDate}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}" />
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                        </lightning:layout>
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="4" padding="around-small">
                                <div class="innertext"><b>General Information</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                            <aura:iteration items="{!v.GeneralInfoFieldSet}" var="field" >
                                <lightning:layoutItem size="6"  padding="horizontal-small">
                                    <aura:if isTrue="{!field.fieldName == 'Contact_Cell_Phone__c'}">
                                        <div id="{!field.fieldName}">
                                            <lightning:inputField aura:id="ContactCellPhone" class="{!field.fieldName}" required="{!v.isRequired}" value="{!field.fieldValue}" fieldName="{!field.fieldName}" disabled="{!v.isInputDisabled}" onchange="{!c.checkPhone}" maxlength = "14"/>
                                        </div>
                                        <aura:set attribute="else"> 
                                            <aura:if isTrue="{!field.fieldName == 'Main_Office_Phone__c'}">
                                                <div id="{!field.fieldName}">
                                                    <lightning:inputField aura:id="MainOfficePhone" class="{!field.fieldName}" required="{!v.isRequired}" value="{!field.fieldValue}" fieldName="{!field.fieldName}" disabled="{!v.isInputDisabled}" onchange="{!c.checkPhone}" maxlength = "14"/>
                                                </div>
                                                <aura:set attribute="else">
                                                    <div id="{!field.fieldName}">
                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" value="{!field.fieldValue}" fieldName="{!field.fieldName}" disabled="{!v.isInputDisabled}"/>
                                                    </div>
                                                </aura:set>
                                            </aura:if>
                                        </aura:set>
                                    </aura:if> 
                                </lightning:layoutItem>
                            </aura:iteration>
                        </lightning:layout>
                        
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="4" padding="around-small">
                                <div class="innertext"><b>Company Information</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                            <aura:iteration items="{!v.CompanyInfoFieldSet}" var="field" >
                                
                                <lightning:layoutItem size="6"  padding="horizontal-small">
                                    <aura:if isTrue="{!field.fieldName == 'Year_Founded_Incorporated__c'}">

                                    <div id="{!field.fieldName}">
                                    <lightning:inputField aura:id="fieldCheck" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}" value="{!field.fieldValue}" disabled="{!v.isInputDisabled}" onchange="{!c.removeComma}"/>
                                        <span style="color:rgb(234,0,30);margin-left:33%;">{!v.validationMessage}</span>
                                    </div>
                                      <aura:set attribute="else">
                                           <div id="{!field.fieldName}">
                                    <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}" value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                    </div>
                                  </aura:set>
                                    </aura:if>
                                </lightning:layoutItem>
                            </aura:iteration>
                        </lightning:layout>
                        
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="4" padding="around-small">
                                <div class="innertext"><b>Key Management</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                            <aura:iteration items="{!v.KeyManageMentSet}" var="field" >
                                
                                <lightning:layoutItem size="6"  padding="horizontal-small">
                                    <!--  <div id="{!field.fieldName}">
                            <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}" disabled="{!v.isInputDisabled}" value="{!field.fieldValue}"/>
                            </div> -->
                                    
                                    <aura:if isTrue="{!or(field.fieldName == 'Key_Management2_Full_Name__c',field.fieldName == 'Key_Management2_Title__c')}">
                                        
                                        <div id="{!field.fieldName}">
                                            <!--lightning:input label="" variant="label-hidden" type="text" name="input1" value="{!v.LVEstimator}" disabled="{!v.isInputDisabled}" aura:id="myinput"/ -->
                                            <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                        </div>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!or(field.fieldName == 'Key_Management2_Total_years_experience__c',field.fieldName == 'Key_Management2_Years_in_this_position__c')}">
                                                <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                
                                                <aura:set attribute="else">
                                                    <aura:if isTrue="{!or(field.fieldName == 'Key_Management3_Full_Name__c',field.fieldName == 'Key_Management3_Title__c')}">
                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                        
                                                        <aura:set attribute="else">
                                                            <aura:if isTrue="{!or(field.fieldName == 'Key_Management3_Total_years_experience__c',field.fieldName == 'Key_Management3_Years_in_this_position__c')}">
                                                                <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                                
                                                                <aura:set attribute="else">
                                                                    <div id="{!field.fieldName}">
                                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                                    </div>
                                                                </aura:set>
                                                            </aura:if> 
                                                        </aura:set>
                                                    </aura:if> 
                                                </aura:set>
                                            </aura:if>   
                                        </aura:set>
                                    </aura:if>   
                                    
                                </lightning:layoutItem>
                            </aura:iteration>
                        </lightning:layout>
                        
                        
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="4" padding="around-small">
                                <div class="innertext"><b>Contractor License Numbers</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                            <aura:iteration items="{!v.ContractorFieldSet}" var="field" >
                                
                                <lightning:layoutItem size="6"  padding="horizontal-small">
                                 <!--   <div id="{!field.fieldName}">
                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}" disabled="{!v.isInputDisabled}" value="{!field.fieldValue}"/>
                                    </div> -->
                                    
                                      <aura:if isTrue="{!or(field.fieldName == 'Contractor_License_Provice_Territory2__c',field.fieldName == 'Contractor_License_Number2__c')}">
                                                <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                
                                                <aura:set attribute="else">
                                                    <aura:if isTrue="{!or(field.fieldName == 'Contractor_License_Provice_Territory3__c',field.fieldName == 'Contractor_License_Number3__c')}">
                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                        
                                                        <aura:set attribute="else">
                                                            <aura:if isTrue="{!or(field.fieldName == 'License_ever_been_denied_or_revoked__c',field.fieldName == 'Complaints_filed_Contractor_s_State_Lice__c')}">
                                                                <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                                
                                                                <aura:set attribute="else">
                                                                    <div id="{!field.fieldName}">
                                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                                    </div>
                                                                </aura:set>
                                                            </aura:if> 
                                                        </aura:set>
                                                    </aura:if> 
                                                </aura:set>
                                            </aura:if>   
                                    
                                </lightning:layoutItem>
                            </aura:iteration>
                        </lightning:layout>
                        
                        
                        
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem size="4" padding="around-small">
                                <div class="innertext"><b>LVE Team</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="8" padding="around-small">
                            </lightning:layoutItem>
                            <aura:iteration items="{!v.LVEFieldSet}" var="field" >
                                
                                <lightning:layoutItem size="6"  padding="horizontal-small">
                                    
                                    <aura:if isTrue="{!field.fieldName == 'If_Other_LVE_Estimator__c'}">
                                        
                                        <div id="{!field.fieldName}">
                                            <!--lightning:input label="" variant="label-hidden" type="text" name="input1" value="{!v.LVEstimator}" disabled="{!v.isInputDisabled}" aura:id="myinput"/ -->
                                            <lightning:inputField aura:id="field" class="{!field.fieldName}" required="false" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                        </div>
                                        <aura:set attribute="else">
                                             <aura:if isTrue="{!field.fieldName == 'Design_Engineer_Cell_Phone__c'}">
                                                <div id="{!field.fieldName}">
                                                    <lightning:inputField aura:id="DesignEngineer1" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"  onchange="{!c.checkPhone}" maxlength = "14"/>
                                                </div>
                                                <aura:set attribute="else">
                                                    <aura:if isTrue="{!field.fieldName == 'Estimator_Cell_Phone__c'}">
                                                        <div id="{!field.fieldName}">
                                                            <lightning:inputField aura:id="EstimatorPhone" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"  onchange="{!c.checkPhone}" maxlength = "14"/>
                                                        </div>
                                                        <aura:set attribute="else">
                                                            <aura:if isTrue="{!field.fieldName == 'Technician1_Cell_Phone__c'}">
                                                                <div id="{!field.fieldName}">
                                                                    <lightning:inputField aura:id="Technician1Phone" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"  onchange="{!c.checkPhone}" maxlength = "14"/>
                                                                </div>
                                                                <aura:set attribute="else">
                                                                    <aura:if isTrue="{!field.fieldName == 'Technician2_Cell_Phone__c'}">
                                                                        <div id="{!field.fieldName}">
                                                                            <lightning:inputField aura:id="Technician2Phone" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"  onchange="{!c.checkPhone}" maxlength = "14"/>
                                                                        </div>
                                                                        <aura:set attribute="else">
                                                                            <aura:if isTrue="{!field.fieldName == 'Foreman_Superindentent_Cell_phone__c'}">
                                                                                <div id="{!field.fieldName}">
                                                                                    <lightning:inputField aura:id="ForemanPhone" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"  onchange="{!c.checkPhone}" maxlength = "14"/>
                                                                                </div>
                                                                                <aura:set attribute="else">
                                                                                    <div id="{!field.fieldName}">
                                                                                        <lightning:inputField aura:id="field" class="{!field.fieldName}" required="{!v.isRequired}" fieldName="{!field.fieldName}"  value="{!field.fieldValue}" disabled="{!v.isInputDisabled}"/>
                                                                                    </div>
                                                                                </aura:set>
                                                                            </aura:if> 
                                                                        </aura:set>
                                                                    </aura:if> 
                                                                </aura:set>
                                                            </aura:if>  
                                                        </aura:set>
                                                    </aura:if>  
                                                </aura:set>
                                            </aura:if>    
                                        </aura:set>
                                    </aura:if>    
                                    
                                </lightning:layoutItem>
                            </aura:iteration>
                        </lightning:layout>
                        
                        
                        
                        
                        
                        
                        <!-- Reference details -->
                        <lightning:layout multipleRows="true">
                            
                            
                            <lightning:layoutItem size="3" padding="around-small">
                                <div><b class="innertext">References</b></div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="9" padding="around-small">
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <div>Reference #1</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <div>Reference #2</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <div>Reference #3</div>
                            </lightning:layoutItem>
                            
                            <lightning:layoutItem size="2" padding="around-small">
                                <div class="inputText"><abbr lightning-groupedcombobox_groupedcombobox="" title="required" class="slds-required">*</abbr>Company Name:</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference1.buildertek__Company_Name__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference2.buildertek__Company_Name__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference3.buildertek__Company_Name__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            
                            <lightning:layoutItem size="2" padding="around-small">
                                <div class="inputText"><abbr lightning-groupedcombobox_groupedcombobox="" title="required" class="slds-required">*</abbr>Address:</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1" value="{!v.reference1.buildertek__Address__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1" value="{!v.reference2.buildertek__Address__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1" value="{!v.reference3.buildertek__Address__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            
                            
                            <lightning:layoutItem size="2" padding="around-small">
                                <div class="inputText"><abbr lightning-groupedcombobox_groupedcombobox="" title="required" class="slds-required">*</abbr>City:</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference1.buildertek__City__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference2.buildertek__City__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:input variant="label-hidden" type="text" name="input1"  value="{!v.reference3.buildertek__City__c}" disabled="{!v.isInputDisabled}" aura:id="GCInput" required="{!v.isRequired}"/>
                            </lightning:layoutItem>
                            
                            
                            <lightning:layoutItem size="2" padding="around-small">
                                <div class="inputText"><abbr lightning-groupedcombobox_groupedcombobox="" title="required" class="slds-required">*</abbr>Province / Territory:</div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="3" padding="around-small">
                                <lightning:select name="mySelect" label="" class="label-hidden" aura:id="GCInput" value="{!v.reference1.Province_Territory__c}" disabled="{!v.isInputDisabled}" required="{!v.isRequired}">
                                    <option value="">Choose one...</option>
                                    <aura:iteration items="{!v.provinceList}" var="company" indexVar="index">
                                        <option text="{!company}" value="{!company}" selected="{!company == v.reference1.Province_Territory__c}"></option>/>
                                    </aura:iteration>
                                </lightning:select>
