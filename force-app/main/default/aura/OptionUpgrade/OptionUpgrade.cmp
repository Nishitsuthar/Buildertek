<aura:component controller="OptionUpgrade" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global">
    <!-- <aura:handler name="init" value="{!this}" action="{!c.doInit}" /> -->

    <div>
        <header>
            <lightning:buttonIcon iconName="utility:close" onclick="{!c.removegroupingcloseModel}" alternativeText="close" variant="bare-inverse" class="slds-modal__close" />
            <h2 id="modal-heading-0111" class="slds-text-heading_medium slds-hyphenate">
                Remove Grouping</h2>
        </header>
        <div class="slds-p-around_medium" id="modal-content-id-11">
            <center>
                <h2>Do you want to use the Product library for this Upgrade ?</h2>
            </center>
        </div>
        <footer>
            <lightning:button variant="neutral" label="No" title="No" onclick="{!c.removegroupingcloseModel}" />
            <lightning:button variant="brand" label="Yes" title="Yes" onclick="{!c.removegroupingBudgetItems}" />
        </footer>
    </div>

</aura:component>