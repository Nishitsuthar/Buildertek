                                                </aura:if>
                                                <aura:if isTrue="{!not(empty(v.logIssueCommonWrapper.Shipping_City))}">
                                                    {!v.logIssueCommonWrapper.Shipping_City}<br />
                                                </aura:if>
                                                
                                                <aura:if
