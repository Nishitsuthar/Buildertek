    <div class="slds-modal__content slds-p-around_medium scrollable" id="modal-content-id-1">
        <div class="slds slds-scope">
            <table class="slds-table slds-table_bordered slds-table_fixed-layout" role="grid">
                <thead>
                    <tr class="slds-text">
                        <th scope="col" style="background-color:rgba(27, 82, 151, 1.0); color:white; border-top-left-radius:5px;vertical-align: top;line-height: 2;width: 30px !important;">
                            <ui:inputCheckbox aura:id="checkContractors" change="{!c.selectAll}" />		
                        </th>
                        <th  style="background-color:rgba(27, 82, 151, 1.0); color:white;vertical-align: top;line-height: 2;width:200">
                            Description
                        </th>
                        <th  style="background-color:rgba(27, 82, 151, 1.0); color:white;vertical-align: top;line-height: 2;width:128px">
                       <!--     Vendor -->
                        </th> 
                        <th  style="background-color:rgba(27, 82, 151, 1.0); color:white;vertical-align: top;line-height: 2;width:200">
                            Quantity
                        </th>
                        <th  style="background-color:rgba(27, 82, 151, 1.0); color:white;vertical-align: top;line-height: 2;width:200">
                            Unit Cost
                        </th>
                    <!--    <th  style="background-color:rgba(27, 82, 151, 1.0); color:white;vertical-align: top;line-height: 2;">
                            Sales Price
                        </th> -->
                    </tr>
                </thead> 
                <tbody>
                    <aura:iteration items="{!v.vendorList}" var="ven">
                        <tr class="groupHeader">
                            <td></td>
                            <td>
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_6-of-12">
                                        <b>{!ven}  </b> 
                                    </div>
                                    <div class="slds-col slds-size_6-of-12 slds-float_right">
                                    </div>
                                </div>
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_1-of-12 slds-is-sortable">
                                    </div>
                                </div>
                            </td>
                            <td></td>
                            <td></td>
                            <td></td>
                         <!--   <td></td> -->
                        </tr>
                        <aura:iteration items="{!v.PaginationList}" var="rec">
                            <aura:if isTrue="{!(ven == rec.VendorName)}">
                                <tr class="slds-hint-parent">
                                    <td>
                                       <!-- <lightning:input aura:id="checkContractor" type="checkbox" value="{!rec.isCheck}" checked="{!rec.isCheck}" name="{!rec.Id}" onchange="{!c.handleCheck}"/> -->
                                                                            <lightning:input aura:id="checkContractor" type="checkbox" value="{!rec.isCheck}" checked="{!rec.isCheck}" name="{!rec.Id}" onchange="{!c.handleCheck}"/>

                                    </td> 
                                    <td role="gridcell" class="slds-truncate">
                                        <ui:outputText value="{!rec.Description}" />  
                                    </td>
                                    <td role="gridcell">
                                   <!--     <ui:outputText value="{!rec.VendorName}"  /> -->
                                    </td> 
                                    <td role="gridcell">
                                        <ui:outputText value="{!rec.Quantity}" />  
                                    </td> 
                                    <td role="gridcell">
                                        <lightning:formattedNumber value="{!rec.Cost}" currencyCode="USD" style="currency" currencyDisplayAs="code"/>
                                    </td>
                                <!--    <td role="gridcell">
                                        <lightning:formattedNumber value="{!rec.Price}" currencyCode="USD" style="currency" currencyDisplayAs="code"/>
                                    </td>  -->
                                </tr>
                            </aura:if>
                        </aura:iteration>
                    </aura:iteration>
                    <tr class="groupHeader">
                        <td></td>
                        <td>
                            <div class="slds-grid slds-gutters">
                                <div class="slds-col slds-size_6-of-12">
                                    <b>No Vendor </b> 
                                </div>
                                <div class="slds-col slds-size_6-of-12 slds-float_right">
                                </div>
                            </div>
                            <div class="slds-grid slds-gutters">
                                <div class="slds-col slds-size_1-of-12 slds-is-sortable">
                                </div>
                            </div>
                        </td>
                        <td></td>
                        <td></td>
                        <td></td>
                     <!--   <td></td>   -->
                    </tr>
                    <aura:iteration items="{!v.PaginationList}" var="rec">
                        <aura:if isTrue="{!(rec.VendorName == undefined )}">
                            <tr class="slds-hint-parent">
                                <td>
                                    <lightning:input aura:id="checkContractor" type="checkbox" value="{!rec.isCheck}" checked="{!rec.isCheck}" name="{!rec.Id}" onchange="{!c.handleCheck}"/>
                                </td> 
                                <td role="gridcell" class="slds-truncate">
                                    <ui:outputText value="{!rec.Description}" />  
                                </td>
                                <td role="gridcell">
                                <!--    <ui:outputText value="{!rec.VendorName}"  /> -->
                                </td> 
                                <td role="gridcell">
                                    <ui:outputText value="{!rec.Quantity}" />  
                                </td> 
                                <td role="gridcell">
                                    <lightning:formattedNumber value="{!rec.Cost}" currencyCode="USD" style="currency" currencyDisplayAs="code"/>
                                </td>
                            <!--    <td role="gridcell">
                                    <lightning:formattedNumber value="{!rec.Price}" currencyCode="USD" style="currency" currencyDisplayAs="code"/>
                                </td>  -->
                            </tr>
                        </aura:if>
                    </aura:iteration>
                </tbody> 
            </table>
            <br/>
        </div> 
        <aura:if isTrue="{!v.Spinner}">
            <div aura:id="spinnerId" class="slds-spinner_container">
                <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                    <span class="slds-assistive-text">Loading</span>
                    <div class="slds-spinner__dot-a"></div>
                    <div class="slds-spinner__dot-b"></div>
                    <div class="custom-loading-text">
                        <b>Processing, Please Wait</b>
                    </div>
                </div>
            </div>
        </aura:if>
    </div>
    <div class="slds-modal__footer">
        <button class="slds-button slds-button--neutral" onclick="{!c.closeModel}" >Cancel</button>
        <button class="slds-button slds-button--brand" onclick="{!c.importBudget}" disabled="{!v.isdisabled}">Create RFQ</button>
    </div>	
    <!--   </div>
    </section> -->
    </aura:if>
</aura:component>